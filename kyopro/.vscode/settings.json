{
  "[cpp]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.path": "/usr/bin/deno@1.35.1",
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "terminal.integrated.fontSize": 20,
  "editor.fontSize": 20,
  "editor.acceptSuggestionOnEnter": "off",
  "window.zoomLevel": 1,
  "files.associations": {
    "*.tcc": "cpp",
    "deque": "cpp",
    "string": "cpp",
    "vector": "cpp",
    "any": "cpp",
    "array": "cpp",
    "bitset": "cpp",
    "ciso646": "cpp",
    "coroutine": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "string_view": "cpp",
    "fstream": "cpp",
    "istream": "cpp",
    "ranges": "cpp",
    "span": "cpp",
    "sstream": "cpp",
    "functional": "cpp",
    "regex": "cpp",
    "type_traits": "cpp",
    "tuple": "cpp",
    "valarray": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "stacktrace": "cpp"
  }
}